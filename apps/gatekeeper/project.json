{
  "root": "apps/gatekeeper",
  "sourceRoot": "apps/gatekeeper",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/gatekeeper",
        "outputPath": "dist/apps/gatekeeper"
      },
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "gatekeeper:build",
        "dev": true,
        "port": 4200,
        "hostname": "dev.local"
      },
      "configurations": {
        "production": {
          "buildTarget": "gatekeeper:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "gatekeeper:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/gatekeeper"
      ],
      "options": {
        "jestConfig": "apps/gatekeeper/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/gatekeeper/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "docker": {
      "executor": "@nx-tools/nx-docker:build",
      "options": {
        "push": false,
        "tags": [
          "dev.local/gatekeeper"
        ]
      },
      "configurations": {
        "production": {
          "push": true,
          "tags": [
            "watheialabs/gatekeeper:v0alpha1"
          ]
        }
      }
    },
    "assemble": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "nx run gatekeeper:build:production",
          "nx run gatekeeper:docker"
        ],
        "parallel": false
      }
    }
  },
  "tags": []
}
