{
  "root": "libs/molecules",
  "sourceRoot": "libs/molecules/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/molecules",
        "tsConfig": "libs/molecules/tsconfig.lib.json",
        "project": "libs/molecules/package.json",
        "entryFile": "libs/molecules/src/index.ts",
        "external": ["react/jsx-runtime", "@mui/material"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "swc",
        "assets": [
          {
            "glob": "libs/molecules/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/molecules/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/molecules"],
      "options": {
        "jestConfig": "libs/molecules/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
